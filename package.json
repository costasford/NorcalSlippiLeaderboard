{
  "name": "react-pages-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate for working with React, redux, react-router and Github Pages",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier 'src/**/**/*.?(js|jsx)' --write",
    "start": "webpack serve",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "deploy": "npm run build && gh-pages -d dist",
    "lint": "eslint src --ext .ts --ext .tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "cross-env NODE_ENV=production webpack --env analyze"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/react-pages-boilerplate.git"
  },
  "author": "Vitaly Rtishchev <rtivital@gmail.com> (http://github.com/rtivital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/react-pages-boilerplate/issues"
  },
  "homepage": "https://costasford.github.io/NorcalSlippiLeaderboard/",
  "dependencies": {
    "buffer": "^6.0.3",
    "classnames": "^2.3.1",
    "css-loader": "^6.7.3",
    "dayjs": "^1.11.7",
    "google-spreadsheet": "^5.0.2",
    "limiter": "^2.1.0",
    "null-loader": "^4.0.1",
    "open-color": "^1.8.0",
    "postcss": "^8.4.20",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.7.1",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^5.1.3",
    "sass-loader": "10",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
